  <div class="btn-group btn-group-toggle" data-toggle="buttons">
  <label class="btn btn-secondary active">
    <input type="radio" name="options" id="option1" autocomplete="off" checked><i class="fa fa-male"></i>
  </label>
  <label class="btn btn-secondary">
    <input type="radio" name="options" id="option2" autocomplete="off"> <i class="fa fa-female"></i>  
  </label>
  </div>
  </div>
  </div>

<button type="button" class="btn btn-primary btn-lg pull-right">Update</button>
</div>




